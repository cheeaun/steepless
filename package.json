{
  "name": "steepless",
  "version": "0.0.1",
  "description": "Find the flattest routes and avoid the hilliest routes",
  "author": "Lim Chee Aun",
  "license": "MIT",
  "scripts": {
    "serve": "serve . -l 8000",
    "js": "esbuild assets/steepless.jsx --bundle --outfile=assets/steepless.js --target=es2018 --legal-comments=none --sourcemap --minify --watch",
    "start": "concurrently \"npm run js\" \"npm run serve\""
  },
  "dependencies": {
    "@deck.gl/core": "~8.7.9",
    "@deck.gl/google-maps": "~8.7.9",
    "@deck.gl/layers": "~8.7.9",
    "react": "~18.1.0",
    "react-dom": "~18.1.0"
  },
  "devDependencies": {
    "concurrently": "~7.2.0",
    "esbuild": "~0.14.39",
    "serve": "~13.0.2"
  }
}
