{
  "name": "steepless",
  "version": "0.0.1",
  "description": "Find the flattest routes and avoid the hilliest routes",
  "author": "Lim Chee Aun",
  "license": "MIT",
  "scripts": {
    "serve": "serve . -l 8000",
    "js": "esbuild assets/steepless.jsx --bundle --outfile=assets/steepless.js --target=es2016 --legal-comments=none --sourcemap --minify --watch",
    "start": "concurrently \"npm run js\" \"npm run serve\""
  },
  "dependencies": {
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "concurrently": "~7.0.0",
    "esbuild": "~0.14.10",
    "serve": "~13.0.2"
  }
}
